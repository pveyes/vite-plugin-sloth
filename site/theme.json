{
  "name": "Sloth",
  "type": "dark",
  "colors": {
    "focusBorder": "#1d1f21",
    "foreground": "#F2F7F2",
    "widget.shadow": "#1d1f21",
    "descriptionForeground": "#F2F7F2",
    "dropdown.background": "#1d1f21",
    "input.background": "#242428",
    "input.border": "#343438",
    "input.foreground": "#F2F7F2",
    "badge.foreground": "#F2F7F2",
    "badge.background": "#0091cd",
    "list.activeSelectionBackground": "#0091cdbb",
    "list.focusBackground": "#E75A7C55",
    "list.hoverBackground": "#242428",
    "activityBar.background": "#1d1f21",
    "activityBar.foreground": "#F2F7F2",
    "activityBar.inactiveForeground": "#575757",
    "activityBarBadge.background": "#0091cd",
    "sideBar.background": "#1d1f21",
    "sideBar.border": "#202024",
    "sideBar.foreground": "#F2F7F2",
    "sideBarTitle.foreground": "#bbbbbb",
    "sideBarSectionHeader.background": "#242428",
    "tab.activeBackground": "#1d1f21",
    "tab.inactiveBackground": "#242428",
    "tab.inactiveForeground": "#696969",
    "editor.background": "#1d1f21",
    "editor.foreground": "#F2F7F2",
    "editorLineNumber.foreground": "#575757",
    "editorLineNumber.activeForeground": "#F2F7F2",
    "editorGutter.addedBackground": "#C3E88D",
    "editorGutter.deletedBackground": "#E75A7C",
    "editorGutter.modifiedBackground": "#F8DDA4",
    "panel.background": "#1d1f21",
    "statusBar.background": "#1d1f21",
    "statusBar.foreground": "#f2f7f2",
    "titleBar.activeBackground": "#1d1f21"
  },
  "tokenColors": [
    {
      "name": "Variables and property access",
      "scope": [
        "variable",
        "meta.block variable.other",
        "string constant.other.placeholder",
        "support.variable.object.node",
        "entity.other.attribute-name.class",
        "source.css variable.interpolation variable",
        "punctuation.accessor",
        "punctuation.definition.dictionary",
        "punctuation.definition.array"
      ],
      "settings": {
        "foreground": "#F2F7F2"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#a7a7a7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation & Braces",
      "scope": [
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.parameters.begin.js",
        "punctuation.definition.parameters.end.js",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution",
        "meta.function.expression",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#878787"
      }
    },
    {
      "name": "Constant & Builtin",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "support.type.primitive",
        "support.type.builtin",
        "source.css variable",
        "constant.character.entity punctuation",
        "storage.type.core.rust"
      ],
      "settings": {
        "foreground": "#f59e0b"
      }
    },
    {
      "name": "String & Regular Expression",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
        "punctuation.definition.string",
        "meta.jsx.children",
        "entity.name.import",
        "string.regexp",
        "string.regexp punctuation"
      ],
      "settings": {
        "foreground": "#adff2f"
      }
    },
    {
      "name": "Native syntax & Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier",
        "entity.name.tag.reference",
        "entity.other.attribute-name",
        "entity.other.attribute-name punctuation",
        "keyword.control.at-rule.media punctuation",
        "meta.assertion",
        "keyword.other",
        "entity.name.lifetime.rust"
      ],
      "settings": {
        "foreground": "#f59e0b"
      }
    },
    {
      "name": "JS API",
      "scope": [
        "support.type.object.module",
        "support.class.builtin",
        "support.class.console",
        "support.class.node",
        "support.constant.math",
        "support.constant.json",
        "support.variable",
        "support.function.builtin"
      ],
      "settings": {
        "foreground": "#A67DB8"
      }
    },
    {
      "name": "Class & Instance",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "entity.other.inherited-class",
        "support.type.sys-types",
        "variable.language",
        "storage.class"
      ],
      "settings": {
        "foreground": "#E75A7C"
      }
    },
    {
      "name": "Function & Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "keyword.other.special-method",
        "support.function"
      ],
      "settings": {
        "foreground": "#eee8aa"
      }
    },
    {
      "name": "HTML Tag, Template & JSX",
      "scope": [
        "entity.name.tag",
        "support.class.component",
        "punctuation.definition.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#ea7c60"
      }
    },
    {
      "name": "Array & Object literal",
      "scope": [
        "meta.array.literal meta.brace.square",
        "meta.objectliteral punctuation.definition.block"
      ],
      "settings": {
        "foreground": "#F2F7F2"
      }
    },
    {
      "name": "Object, JSON key, and HTML attributes",
      "scope": [
        "meta.object-literal.key",
        "meta.object-literal.key string",
        "meta.object-literal.key punctuation.definition.string",
        "meta.type.tuple punctuation.definition.string",
        "meta.type.tuple string",
        "meta.tag.attributes entity.other.attribute-name",
        "meta.tag",
        "meta.attribute entity.other.attribute-name",
        "variable.object.property",
        "meta.embedded.block.frontmatter entity.name.tag.yaml",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#eee8aa"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown content",
      "scope": ["text.html.markdown punctuation"],
      "settings": {
        "foreground": "#F2F7F2"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "punctuation.definition.heading",
        "markup.heading punctuation",
        "markup.heading entity.name"
      ],
      "settings": {
        "foreground": "#E75A7C"
      }
    },
    {
      "name": "Markdown - Bold",
      "scope": ["markup.bold", "punctuation.definition.bold"],
      "settings": {
        "foreground": "#DBB9F1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Italic",
      "scope": ["markup.italic", "punctuation.definition.italic"],
      "settings": {
        "foreground": "#DBB9F1",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Code",
      "scope": ["markup.inline.raw", "punctuation.definition.raw"],
      "settings": {
        "foreground": "#DBB9F1"
      }
    }
  ]
}
